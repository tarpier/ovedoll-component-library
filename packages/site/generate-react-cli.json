{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "index": "./templates/component/index.ts",
        "component": "./templates/component/TemplateName.tsx",
        "story": "./templates/component/TemplateName.stories.tsx"
      },
      "path": "src/components",
      "withIndex": true,
      "withStory": true,
      "withStyle": false,
      "withTest": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "./templates/page/TemplateName.tsx"
      },
      "path": "src/pages",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}